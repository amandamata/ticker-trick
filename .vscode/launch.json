{
    "version": "0.2.0",
    "configurations": [
        {
          "name": "Launch Frontend",
          "type": "chrome",
          "request": "launch",
          "url": "http://localhost:3000",
          "webRoot": "${workspaceFolder}/ticker-trick-frontend",
          "sourceMapPathOverrides": {
            "webpack:///src/*": "${webRoot}/*"
          }
        },
        {
            "name": "Launch Api",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/ticker-trick-backend/main.go"
        }
    ]
}
